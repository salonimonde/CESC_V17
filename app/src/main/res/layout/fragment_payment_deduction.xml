<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/background_color"
    android:orientation="vertical">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/text_background_2"
        android:visibility="visible">


        <TextView
            android:id="@+id/lbl_total_deduction"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/text_background_2"
            android:hint="Total Deduction"
            android:padding="5dp"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_large" />

        <TextView
            android:id="@+id/txt_total_deduction"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/text_background_2"
            android:hint="none"
            android:padding="5dp"
            android:textColor="@color/text_color"
            android:textSize="18sp" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/text_background_2">

        <TextView
            android:id="@+id/lbl_pf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/text_background_2"
            android:hint="PF"
            android:padding="5dp"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_large" />

        <TextView
            android:id="@+id/txt_pf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lbl_pf"
            android:layout_marginLeft="5dp"
            android:background="@drawable/text_background_2"
            android:hint="none"
            android:padding="5dp"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_large" />


        <TextView
            android:id="@+id/lbl_esi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/txt_pf"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="15dp"
            android:hint="ESI"
            android:padding="5dp"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_large" />


        <TextView
            android:id="@+id/txt_esi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/lbl_esi"
            android:layout_marginLeft="5dp"
            android:hint="none"
            android:padding="5dp"
            android:layout_marginBottom="5dp"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_large" />

        <TextView
            android:id="@+id/lbl_other"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_pftax"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="15dp"
            android:hint="Other"
            android:padding="5dp"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_large" />


        <TextView
            android:id="@+id/txt_other"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lbl_other"
            android:layout_marginLeft="200dp"
            android:hint="none"
            android:layout_marginBottom="5dp"
            android:padding="5dp"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_large" />

        <!-- <TextView
            android:id="@+id/lbl_allocated"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_binder"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/text_background_2"
            android:hint="Allocated"
            android:padding="5dp"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_large" />

        <TextView
            android:id="@+id/txt_allocated"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lbl_allocated"
            android:layout_marginLeft="5dp"
            android:background="@drawable/text_background_2"
            android:hint="250"
            android:padding="5dp"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_large" />

        <TextView
            android:id="@+id/lbl_readings"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_consumers"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/text_background_2"
            android:hint="Readings"
            android:padding="5dp"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_large" />


        <TextView
            android:id="@+id/txt_reading"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lbl_readings"
            android:layout_marginLeft="200dp"
            android:background="@drawable/text_background_2"
            android:hint="5000"
            android:padding="5dp"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_large" />

        <TextView
            android:id="@+id/lbl_sitenotfound"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_allocated"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="15dp"
            android:hint="Site Not Found"
            android:padding="5dp"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_large" />

        <TextView
            android:id="@+id/txt_sitenotfound"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lbl_sitenotfound"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="5dp"
            android:background="@drawable/text_background_2"
            android:hint="34"
            android:padding="5dp"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_large" />

        <TextView
            android:id="@+id/lbl_rnt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_reading"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/text_background_2"
            android:hint="RNT"
            android:padding="5dp"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_large" />

        <TextView
            android:id="@+id/txt_rnt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lbl_rnt"
            android:layout_marginLeft="200dp"
            android:background="@drawable/text_background_2"
            android:hint="56"
            android:padding="5dp"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_large" />-->

        <TextView
            android:id="@+id/lbl_pftax"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="10dp"
            android:hint="PTax "
            android:padding="5dp"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_large" />


        <TextView
            android:id="@+id/txt_pftax"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lbl_pftax"
            android:layout_marginLeft="200dp"
            android:hint="none"
            android:padding="5dp"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_large" />

    </RelativeLayout>
</LinearLayout>
